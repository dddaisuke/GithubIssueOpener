{
  "name": "GitHub Issue Opener",
  "author": "Daisuke Yamashita (@dddaisuke)",
  "description": "This is a Chrome extension for jump to issue on GitHub.",
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://github.com/"
  ],
  "version": "1.0.0",
  "manifest_version": 2
}
